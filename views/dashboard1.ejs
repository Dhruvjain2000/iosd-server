<div class="container container-home p-t-md">
    <div class="row">
        <div class="col-md-3 col-lg-2">
            <div class="panel panel-default panel-profile m-b-md">
                <div class="panel-body text-center">
                    <a href="dashboard">
                        <img
                                class="panel-profile-img"
                                src="/<%= user.picture %>">
                    </a>

                    <h5 class="panel-title">
                        <a class="text-inherit"><%= user.name %></a>
                    </h5>

                    <p class="m-b-md"><%= user.college %></p>

                    <ul class="panel-menu">
                        <li class="panel-menu-item">
                            <a class="text-inherit">
                                Member
                            </a>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="panel panel-default visible-md-block visible-lg-block">
                <div class="panel-body">
                    <h5 class="m-t-0">About
                        <small>· <a href="#"></a></small>
                    </h5>
                    <ul class="list-unstyled list-spaced">
                        <li><i class="fa fa-university" aria-hidden="true"></i>&ensp;Went to <a
                                    href="#"><%= user.college %></a></li>
                        <li><i class="fa fa-calendar" aria-hidden="true"></i></span>&ensp;Member
                            Since <%= user.joinDate %></li>
                        <!-- <li><span class="text-muted icon icon-github m-r"></span>&ensp;<a href="#">/<%= user.picture %></a></li> -->
                        <li><i class="fa fa-twitter" aria-hidden="true"></i>&emsp;<a href="#"><%= user.twitter %></a>
                        </li>
                        <li><i class="fa fa-facebook" aria-hidden="true"></i>&emsp;<a href="#"><%= user.facebook %></a>
                        </li>
                        <li><i class="fa fa-linkedin" aria-hidden="true"></i>&emsp;<a href="#"><%= user.linkedin %></a>
                        </li>
                        <!-- <li><span class="text-muted icon icon-home m-r"></span>&ensp;Lives in <a href="#">New Delhi</a></li> -->
                    </ul>
                </div>
            </div>
            <div class="panel panel-default m-b-md hidden-xs">
                <div class="panel-body">
                    <h5 class="m-t-0">Events</h5>
                    <div data-grid="images" data-target-height="150">
                        <img class="media-object" data-width="640" data-height="640" data-action="zoom"
                             src="images/dashboard/hackdtu.jpg">
                    </div>
                    <p><strong>HACKDTU 2.0</strong></p>
                    <a href="http://www.hackdtu.tech" class="btn btn-primary-outline btn-sm">Register Now!</a>
                </div>
            </div>

            <div class="panel panel-default m-b-md hidden-xs">
                <div class="panel-body">
                    <h5 class="m-t-0">Mentors
                        <small>· <a href="#">View All</a></small>
                    </h5>
                    <ul class="media-list media-list-stream">
                        <li class="media m-b">
                            <a class="media-left" href="#">
                                <img
                                        class="media-object img-circle"
                                        src="images/dashboard/krishnarai.jpg">
                            </a>
                            <div class="media-body">
                                <strong>Krishna Rai</strong>&ensp;@RAI.KRISHNA
                                <div class="media-body-actions">
                                    <a class="btn btn-primary-outline btn-sm"
                                       href="https://www.facebook.com/RAI.KRISHNA">
                                        <span class="icon icon-phone"></span>Contact</a>
                                </div>
                            </div>
                        </li>
                        <li class="media">
                            <a class="media-left" href="#">
                                <img
                                        class="media-object img-circle"
                                        src="images/dashboard/nikhilpandey.jpg">
                            </a>
                            <div class="media-body">
                                <strong>Nikhil Pandey</strong>&ensp;@menikhilpandey
                                <div class="media-body-actions">
                                    <a class="btn btn-primary-outline btn-sm"
                                       href="https://www.facebook.com/menikhilpandey">
                                        <span class="icon icon-phone"></span>Conact</a>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>


        </div>

        <div class="col-md-5 col-lg-6">
            <ul class="list-group media-list media-list-stream">

                <li class="media list-group-item p-a">
                    <a id='dwnldmembercard' class="btn btn-primary-outline btn-sm">Download Membership Card</a>
                </li>

                <li class="media list-group-item p-a">
                    <div class="container" style="width:auto">
                        <img class='img-responsive' src='<%= idcard %>'></img>
                    </div>
                </li>
            </ul>
            <div class="panel panel-default">
                <div class="panel-body">
                    <div><h4>Github Feed</h4></div>
                    <div id="root">

                    </div>
                </div>
            </div>

        </div>

        <div class="col-md-4 a">
            <div class="panel panel-default visible-md-block visible-lg-block">
